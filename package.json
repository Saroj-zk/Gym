{
  "name": "gymstack",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "services/*"
  ],
  "scripts": {
    "dev": "concurrently -k \"npm:dev:api\" \"npm:dev:admin\" \"npm:dev:member\"",
    "dev:api": "npm --workspace services/api run dev",
    "dev:worker": "npm --workspace services/worker run dev",
    "dev:admin": "npm --workspace apps/admin-web run dev",
    "dev:member": "npm --workspace apps/member-app run dev",
    "build": "npm -ws run build",
    "lint": "npm -ws run lint",
    "typecheck": "npm -ws run typecheck"
  },
  "devDependencies": {
    "concurrently": "^9.0.0",
    "typescript": "^5.5.4"
  }
}
